#include <stdio.h>
int main() {
    int a;
    printf("Vvedite znachenie dlya a ");
    scanf("%d", &a);
    int b;
    printf("Vvedite znachenie dlya b ");
    scanf("%d", &b);
    int c;
    printf("Vvedite znachenie dlya c ");
    scanf("%d", &c);
    int V = a*b*c;
    printf("V = %d \n", V);
    return 0;
}
