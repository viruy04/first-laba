#include <stdio.h>
#include <math.h>
int main() {
    float x1, y1, x2, y2;
    printf("Vvedite znachenie dlya x1 ");
    scanf("%f", &x1);
    printf("Vvedite znachenie dlya y1 ");
    scanf("%f", &y1);
    printf("Vvedite znachenie dlya x2 ");
    scanf("%f", &x2);
    printf("Vvedite znachenie dlya y2: ");
    scanf("%f", &y2);
    float length = sqrt((x2 - x1) * (x2 - x1) + (y2 - y1) * (y2 - y1));
    printf("Rasstoyanie mezdu tochkami: %.2f\n", length);
    return 0;
}
